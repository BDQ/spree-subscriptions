<%= error_messages_for :creditcard %>
<h1><%= t("editing_creditcard") %></h1>
<br/>
<% form_for(:creditcard, :url => subscription_creditcard_url, :html => { :method => :put }) do |f| %>
  <%= render :partial => "form", :locals => { :f => f } %>
  <p>
    <%=submit_tag t("update") %>
  </p>
<% end %>

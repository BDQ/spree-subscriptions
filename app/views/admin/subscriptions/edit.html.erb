<h1><%= t("editing_subscription") %></h1>

<%= render :partial => 'admin/shared/subscription_tabs', :locals => {:current => "Subscription Details"} %>

<% form_for(:subscription, :url => object_url, :html => { :method => :put }) do |f| %>
  <%= render :partial => "form", :locals => { :f => f } %>
  <p class="form-buttons">
    <%= button t("update") %> <%= link_to t('cancel'), collection_url %>
  </p>
<% end %>
